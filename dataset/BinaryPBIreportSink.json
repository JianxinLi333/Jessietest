{
	"name": "BinaryPBIreportSink",
	"properties": {
		"linkedServiceName": {
			"referenceName": "AzureBlobStorage1",
			"type": "LinkedServiceReference"
		},
		"annotations": [],
		"type": "Binary",
		"typeProperties": {
			"location": {
				"type": "AzureBlobStorageLocation",
				"fileName": {
					"value": "@concat('Repoxt_', formatDateTime(utcnow(), 'yyyy-MM-ddTHH:mm:ss'),'.pbix')",
					"type": "Expression"
				},
				"container": "output"
			},
			"compression": {
				"type": "ZipDeflate"
			}
		}
	}
}